openbas:
  url: 'http://localhost:3001'
  token: 'ChangeMe'

collector:
  id: 'changeme'
  name: 'Amazon Web Services'
  period: 3600
  log_level: 'info'
  aws_access_key_id: ''  # AWS Access Key ID (optional if using instance role)
  aws_secret_access_key: ''  # AWS Secret Access Key (optional if using instance role)
  aws_session_token: ''  # AWS Session Token (optional, for temporary credentials)
  aws_assume_role_arn: ''  # ARN of role to assume (optional)
  aws_regions: ''  # Comma-separated list of regions, leave empty to discover all regions
